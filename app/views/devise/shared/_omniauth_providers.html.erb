<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to raw("<i class='fa fa-sign-in'></i> Sign in with #{OmniAuth::Utils.camelize(provider)}"), omniauth_authorize_path(resource_name, provider),class: "btn btn-success btn-block btn-lg",style: "margin-left: 5px;",width: "100px;" %><br />
  <% end -%>
<% end -%>
